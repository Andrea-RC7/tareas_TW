},{
  nombre: "Erik Magnus Lehnsherr",
  edad: 49,
  mutante: true
},{
  nombre: "James Logan",
  edad: undefined,
  mutante: true
}];

type superpersonas ={
        
        poder:String;
        estatura:number;

    }

const charles: superpersonas = {
  poder:"psiquico",
  estatura: 1.78
};

